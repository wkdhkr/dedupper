{
  "name": "dedupper",
  "version": "2.1.0",
  "description": "import various files, detect duplicates with sqlite database.",
  "license": "MIT",
  "author": "wkdhkr",
  "main": "index.js",
  "bin": {
    "dedupper": "bin/dedupper.js",
    "facepptest": "bin/facepptest.js",
    "hdcalc": "bin/hdcalc.js",
    "nsfwcheck": "bin/nsfwcheck.js",
    "facecheck": "bin/facecheck.js",
    "facespinnertest": "bin/facespinnertest.js",
    "nsfwjstest": "bin/nsfwjstest.js",
    "faceapitest": "bin/faceapitest.js",
    "posenettest": "bin/posenettest.js",
    "cocossdtest": "bin/cocossdtest.js",
    "photosift": "bin/photosift.js"
  },
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src -d dist",
    "build:watch": "cross-env NODE_ENV=production babel --watch src -d dist --verbose",
    "clean": "rimraf coverage dist tmp",
    "codecov": "codecov",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "lint": "eslint .",
    "postlint": "npm run typecheck",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage --maxWorkers=2",
    "test:watch": "jest --watch --maxWorkers=4",
    "typecheck": "flow check"
  },
  "dependencies": {
    "@babel/preset-flow": "^7.0.0",
    "@tensorflow-models/coco-ssd": "^2.0.0",
    "@tensorflow-models/posenet": "^2.1.3",
    "@tensorflow/tfjs-converter": "^1.2.7",
    "@tensorflow/tfjs-core": "^1.2.7",
    "@tensorflow/tfjs-data": "^1.2.7",
    "@tensorflow/tfjs-layers": "^1.2.7",
    "@tensorflow/tfjs-node": "^1.2.9",
    "@tensorflow/tfjs-node-gpu": "^1.2.9",
    "app-root-path": "^2.2.1",
    "async-csv": "^2.1.1",
    "await-sleep": "^0.0.1",
    "axios": "^0.19.0",
    "axios-retry": "^3.1.2",
    "canvas-prebuilt": "2.0.0-alpha.9",
    "chalk": "^2.4.2",
    "child-process-promise": "^2.2.1",
    "commander": "^3.0.1",
    "concat-stream": "^2.0.0",
    "delete-empty": "^3.0.0",
    "dhash-image": "^1.2.1",
    "face-api.js": "^0.20.1",
    "fast-text-encoding": "^1.0.0",
    "follow-redirects": "^1.7.0",
    "form-data": "^2.5.1",
    "fs-extra": "^8.1.0",
    "glob-promise": "^3.4.0",
    "jimp": "^0.6.4",
    "lockfile": "^1.0.4",
    "log4js": "^4.5.1",
    "max-listeners-exceeded-warning": "^0.0.1",
    "mkdirp": "^0.5.1",
    "mv": "^2.1.1",
    "nsfwjs": "^2.1.0",
    "opencv4nodejs": "^5.2.0",
    "opencv4nodejs-prebuilt": "^4.15.2",
    "os-utils": "^0.0.14",
    "p-limit": "^2.2.1",
    "phash": "git+https://github.com/wkdhkr/node-phash.git#master",
    "pify": "^4.0.1",
    "qs": "^6.8.0",
    "recursive-readdir": "^2.2.2",
    "require-uncached": "^2.0.0",
    "rimraf": "^2.6.3",
    "root-require": "^0.3.1",
    "sharp": "0.20.1",
    "source-map-support": "^0.5.13",
    "sqlite3": "^4.0.9",
    "strip-ansi": "^5.2.0",
    "test-audio": "^2.1.0",
    "tfjs-image-recognition-base": "^0.6.1",
    "tmp-promise": "^2.0.2",
    "touch": "^3.1.0",
    "trash": "^6.0.0",
    "wait-on": "^4.0.0",
    "winattr": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-proposal-do-expressions": "7.5.0",
    "@babel/plugin-proposal-export-default-from": "7.5.2",
    "@babel/plugin-proposal-export-namespace-from": "7.5.2",
    "@babel/plugin-proposal-function-bind": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.5.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.4.4",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "7.5.0",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "7.5.0",
    "@babel/polyfill": "7.4.4",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-flow": "7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-minify": "^0.5.1",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "babel-plugin-source-map-support": "^2.1.1",
    "babel-preset-minify": "^0.5.0",
    "babel-upgrade": "1.0.1",
    "codecov": "^3.5.0",
    "cross-env": "^5.2.1",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "flow-bin": "^0.107.0",
    "flow-typed": "2.5.1",
    "jest": "^24.9.0",
    "license-checker": "^25.0.1",
    "node-pre-gyp": "^0.13.0",
    "prettier": "^1.18.2",
    "prettier-plugin-package": "^0.3.1"
  },
  "babel": {
    "ignore": [
      "./dist/**/*"
    ],
    "presets": [
      "@babel/preset-flow",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          },
          "modules": false
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-modules-commonjs",
      "dynamic-import-node-babel-7",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-logical-assignment-operators",
      "@babel/plugin-proposal-optional-chaining",
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "minimal"
        }
      ],
      "@babel/plugin-proposal-nullish-coalescing-operator",
      "@babel/plugin-proposal-do-expressions",
      "@babel/plugin-proposal-function-bind"
    ],
    "env": {
      "production": {
        "presets": []
      },
      "test": {
        "plugins": [
          "./istanbulHackPlugin"
        ]
      }
    }
  },
  "engineStrict": true,
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 85,
        "lines": 80,
        "statements": 80
      }
    },
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/",
      "dist/"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "src/services/deepLearning/CocoSsdService.js",
      "src/services/deepLearning/FaceSpinnerService.js",
      "src/services/deepLearning/faceApi/",
      "src/services/deepLearning/poseNet"
    ]
  }
}
