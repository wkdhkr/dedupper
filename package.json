{
  "name": "dedupper",
  "version": "1.0.0",
  "bin": {
    "dedupper": "bin/dedupper.js",
    "hdcalc": "bin/hdcalc.js",
    "nsfwcheck": "bin/nsfwcheck.js",
    "facecheck": "bin/facecheck.js"
  },
  "description": "import various files, detect duplicates with sqlite database.",
  "engineStrict": true,
  "engines": {
    "node": ">=8.9"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "main": "index.js",
  "scripts": {
    "clean": "rimraf coverage dist tmp",
    "lint": "eslint .",
    "postlint": "npm run typecheck",
    "typecheck": "flow check",
    "pretest": "npm run lint",
    "test": "npm run test-only",
    "test-only": "jest --coverage",
    "test:watch": "jest --watch --maxWorkers=4",
    "build": "babel src -d dist",
    "build:watch": "babel --watch src -d dist",
    "flow": "flow",
    "flow-typed": "flow-typed"
  },
  "author": "wkdhkr",
  "license": "MIT",
  "dependencies": {
    "await-sleep": "0.0.1",
    "axios": "^0.18.0",
    "chalk": "^2.3.2",
    "child-process-promise": "^2.2.1",
    "commander": "^2.14.1",
    "concat-stream": "^1.6.1",
    "delete-empty": "^2.0.0",
    "dhash-image": "^1.2.1",
    "follow-redirects": "^1.4.1",
    "form-data": "^2.3.2",
    "fs-extra": "^5.0.0",
    "imagemin": "^5.3.1",
    "imagemin-jpegoptim": "5.2.0",
    "imagemin-optipng": "5.2.1",
    "lockfile": "^1.0.3",
    "log4js": "^2.5.3",
    "max-listeners-exceeded-warning": "^0.0.1",
    "mkdirp": "^0.5.1",
    "p-limit": "^1.2.0",
    "phash": "git+https://github.com/wkdhkr/node-phash.git#master",
    "pify": "^3.0.0",
    "recursive-readdir": "^2.2.2",
    "require-uncached": "^1.0.3",
    "rimraf": "^2.6.2",
    "root-require": "^0.3.1",
    "sharp": "^0.19.1",
    "sqlite3": "^3.1.13",
    "strip-ansi": "^4.0.0",
    "tmp": "^0.0.33",
    "touch": "^3.1.0",
    "trash": "^4.3.0",
    "winattr": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.40",
    "@babel/core": "^7.0.0-beta.40",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0-beta.40",
    "@babel/polyfill": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "@babel/preset-flow": "^7.0.0-beta.40",
    "@babel/preset-stage-0": "^7.0.0-beta.40",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.6",
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "flow-bin": "0.59.0",
    "flow-typed": "^2.3.0",
    "jest": "^22.4.2",
    "license-checker": "^16.0.0",
    "prettier": "^1.11.1"
  }
}
